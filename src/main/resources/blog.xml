<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<comment>Pandoc的安装目录</comment>
	<entry key="pandoc_home">F:/pandoc-2.1/</entry>
	<entry key="word_basepath">G:/git-local/blog/</entry>
	<!-- Markdown文档的存储根目录,当你的博客是直接采用Markdown格式编写的,此时你可以配置此参数,它与word_basepath参数二选一即可 -->
	<entry key="markdown_basepath">G:/git-local/blog/</entry>
	<!-- 多个日期请使用分号;进行分割 -->
	<entry key="blog_send_dates">20180117</entry>
	<!-- 当前博客发布工具支持的博客平台,多个博客平台名称请使用分号;进行分割 -->
	<entry key="blog_platform_supported">github;CSDN</entry>

	<!-- Github的本地仓库目录 -->
	<entry key="github_local_repo_path">G:/git-local</entry>
	<!-- Github的远程仓库的URL -->
	<entry key="github_remote_repo_path">https://github.com/yida-lxw/blog.git</entry>
	<!-- Github博客的远程分支名称 -->
	<entry key="github_blog_branch_name">master</entry>
	<!-- 本地项目代码存放目录-->
	<entry key="github_local_code_dir">G:/git-local/blog/</entry>

	<!-- Github本地私钥存放路径,若未指定或留空,则默认值为C:/Users/Administrator/.ssh/id_rsa-->
	<entry key="github_private_key_path"></entry>
	<!-- Github登录账号-->
	<entry key="github_username">yida-lxw</entry>
	<!-- Github登录密码 -->
	<entry key="github_pwd">1234567890</entry>

	<!-- Github提交操作设置的作者和提交者的姓名信息-->
	<entry key="github_author_name">Lanxiaowei</entry>
	<!-- Github提交操作设置的作者和提交者的邮箱地址信息-->
	<entry key="github_author_email">736031305@qq.com</entry>
</properties>